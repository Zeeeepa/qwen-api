# ==============================================
# Qwen API Server Configuration
# ==============================================

# Server Configuration
LISTEN_PORT=8080
HOST=0.0.0.0
SERVICE_NAME=qwen-ai2api-server

# Logging
DEBUG_LOGGING=false

# Security & Authentication
ANONYMOUS_MODE=true
SKIP_AUTH_TOKEN=false

# Token Pool (Optional - for API key rotation)
# Path to file containing authentication tokens (one per line or comma-separated)
AUTH_TOKENS_FILE=

# Token pool health configuration
TOKEN_HEALTH_CHECK_INTERVAL=300
TOKEN_FAILURE_THRESHOLD=3
TOKEN_RECOVERY_TIMEOUT=1800

# FlareProx Configuration (for IP rotation and load balancing)
FLAREPROX_ENABLED=false
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=
FLAREPROX_PROXY_COUNT=3
FLAREPROX_AUTO_ROTATE=true

# Provider Credentials
# Z.AI / Qwen Provider
QWEN_EMAIL=
QWEN_PASSWORD=

# K2Think Provider (if applicable)
K2THINK_EMAIL=
K2THINK_PASSWORD=

# LongCat Provider (if applicable)
LONGCAT_EMAIL=
LONGCAT_PASSWORD=

# Model Configuration
PRIMARY_MODEL=GLM-4.5
THINKING_MODEL=GLM-4.5-Thinking
SEARCH_MODEL=GLM-4.5-Search

# API Configuration
API_ENDPOINT=https://chat.qwen.ai/api/chat/completions

# Features
TOOL_SUPPORT=true
SCAN_LIMIT=200000

# Retry Configuration
MAX_RETRIES=5
RETRY_DELAY=1.0

# ==============================================
# Advanced Configuration
# ==============================================

# Request Tracking
REQUEST_TRACKER_MAX_TRACKED=10000
REQUEST_TRACKER_CLEANUP_INTERVAL=300
REQUEST_TRACKER_MAX_REQUEST_AGE=3600

# HTTP Client
HTTP_CLIENT_TIMEOUT=30.0
HTTP_CLIENT_MAX_RETRIES=3
HTTP_CLIENT_RETRY_DELAY=1.0
HTTP_CLIENT_MAX_CONNECTIONS=100
HTTP_CLIENT_MAX_KEEPALIVE=50

# ==============================================
# Docker-specific Configuration
# ==============================================

# These are typically only used when running via docker-compose
# Set them in docker-compose.yml or docker-compose.override.yml

